{
    "name": "collective-thinking/laravel-seed",
    "description": "Run your seeds like you run your migrations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Khalyomede",
            "email": "khalyomede@gmail.com"
        },
        {
            "name": "Collective Thinking",
            "email": "tech@collective-thinking.com"
        }
    ],
    "keywords": [
        "laravel",
        "seed",
        "migration",
        "batch"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0",
        "jawira/case-converter": "3.*",
        "laravel/framework": "8.*"
    },
    "autoload": {
        "psr-4": {
            "CollectiveThinking\\LaravelSeed\\": "src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.4.0",
        "phpstan/phpstan": "1.10.57"
    },
    "scripts": {
        "lint": "php-cs-fixer fix --diff --using-cache=no --allow-risky=yes --dry-run",
        "format": "php-cs-fixer --using-cache=no --allow-risky=yes fix",
        "analyse": "phpstan analyse"
    }
}
